  },
  "homepage": "https://github.com/HikaruYasuda/markdown-uml-js#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "node-sass": "^3.7.0",
    "watch": "^0.18.0"
  }
}
